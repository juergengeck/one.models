{
    "compilerOptions": {
        "noEmit": false,
        "outDir": "../lib",
        "isolatedModules": false,
        "moduleResolution": "node",
        "sourceMap": false,
        "emitDeclarationOnly": true,
        "declaration": true,
        "declarationMap": true,
        "downlevelIteration": false,
        "importHelpers": false,
        "esModuleInterop": true,
        "importsNotUsedAsValues": "error",
        "composite": true,
        "tsBuildInfoFile": "../tsconfig.plan.tsbuildinfo",
        "lib": ["esnext", "dom"],
        "module": "esnext",
        "target": "esnext",
        "alwaysStrict": true,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "noImplicitReturns": true,
        "strict": true,
        "newLine": "LF",
        "allowJs": true,
        "checkJs": true,
        "removeComments": false,
        "forceConsistentCasingInFileNames": true
    },
    "include": ["./*.ts", "./**/*.ts"],
    "exclude": [],
    "references": [
        {
            "path": "../src"
        }
    ]
}
