{
    "compilerOptions": {
        "noEmit": true,
        "outDir": "lib",
        "isolatedModules": false,
        "moduleResolution": "node",
        "sourceMap": false,
        "emitDeclarationOnly": false,
        "declaration": true,
        "declarationMap": true,
        "downlevelIteration": false,
        "importHelpers": false,
        "esModuleInterop": true,
        "importsNotUsedAsValues": "error",
        "composite": true,
        "tsBuildInfoFile": "./tsconfig.tsbuildinfo",
        "lib": [
            "esnext",
            "dom"
        ],
        "module": "esnext",
        "target": "esnext",
        "alwaysStrict": true,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "noImplicitReturns": true,
        "strict": true,
        "newLine": "LF",
        "allowJs": true,
        "checkJs": true,
        "removeComments": false,
        "forceConsistentCasingInFileNames": true,
        "baseUrl": "./",
        "paths": {
        }
    },
    "references": [
        {
            "path": "./src"
        },
        {
            "path": "./test"
        }
    ],
    "include": [
        "*.js"
    ],
    "exclude": [
        "node_modules/one.core/src",
        ".*.js",
        "doc"
    ]
}
